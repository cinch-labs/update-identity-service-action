{
  "name": "update-identity-service",
  "version": "1.1.0",
  "private": true,
  "description": "GitHub action to update subdomains on the identity service",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint:check": "eslint 'src/*.ts'",
    "lint:fix": "eslint 'src/*.ts' --fix",
    "package": "ncc build --source-map",
    "prettier:check": "prettier \"./**/*\" --check",
    "prettier:fix": "prettier \"./**/*\" --check --write",
    "test": "jest",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinch-labs/update-identity-service.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "axios": "0.23.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^26.4.2",
    "jest-circus": "^27.2.5",
    "js-yaml": "^4.1.0",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.1",
    "ts-jest": "^26.2.0",
    "typescript": "^4.4.4"
  }
}

{
  "name": "update-identity-service",
  "version": "1.0.1",
  "private": true,
  "description": "GitHub action to update subdomains on the identity service",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint:check": "eslint 'src/*.ts'",
    "lint:fix": "eslint 'src/*.ts' --fix",
    "package": "ncc build --source-map",
    "prettier:check": "prettier \"./**/*\" --check",
    "prettier:fix": "prettier \"./**/*\" --check --write",
    "release": "standard-version",
    "release-major": "standard-version --release-as major",
    "release-minor": "standard-version --release-as minor",
    "release-patch": "standard-version --release-as patch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinch-labs/update-identity-service.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.25",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "jest-circus": "^26.4.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  }
}

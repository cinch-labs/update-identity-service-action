{
  "name": "update-identity-service",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub action to update subdomains on the identity service",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint:check": "eslint 'src/*.ts'",
    "lint:fix": "eslint 'src/*.ts' --fix",
    "prettier:fix": "prettier \"./**/*\" --check --write",
    "prettier:check": "prettier \"./**/*\" --check",
    "package": "ncc build --source-map",
    "test": "jest",
    "all": "npm run build && npm run lint:check && npm run prettier:check && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinch-labs/update-identity-service.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4"
  },
  "devDependencies": {
    "@cinch-labs/eslint-config": "^1.4.0",
    "@cinch-labs/prettier-config": "1.0.0",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@typescript-eslint/parser": "^3.9.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.5.0",
    "eslint-plugin-github": "^4.1.1",
    "husky": "^4.2.5",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "ts-jest": "^24.3.0",
    "typescript": "^3.9.7"
  }
}
